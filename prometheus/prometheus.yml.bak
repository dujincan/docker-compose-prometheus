global:
  scrape_interval:     15s
  evaluation_interval: 15s

  # Attach these labels to any time series or alerts when communicating with
  # external systems (federation, remote storage, Alertmanager).
  external_labels:
      monitor: 'docker-host-alpha'

# Load and evaluate rules in this file every 'evaluation_interval' seconds.
rule_files:
  - "alert.rules"

# A scrape configuration containing exactly one endpoint to scrape.
scrape_configs:
  - job_name: 'jummpserver_node_exporter'
    scrape_interval: 5s
    static_configs:
      - targets: ['nodeexporter:9100']

  - job_name: 'Prod_DockerRose_node_exporter'
    scrape_interval: 5s
    static_configs:
      - targets: ['10.64.4.155:9100']

  - job_name: 'Prod_DockerJack_node_exporter'
    scrape_interval: 5s
    static_configs:
      - targets: ['10.64.4.146:9100']

  - job_name: 'jason_test_node_exporter'
    scrape_interval: 5s
    static_configs:
      - targets: ['101.37.77.148:9100']

  - job_name: 'tomcat1_node_exporter'
    scrape_interval: 5s
    static_configs:
      - targets: ['121.196.197.231:9100']

  - job_name: 'tomcat2_node_exporter'
    scrape_interval: 5s
    static_configs:
      - targets: ['121.43.163.194:9100']
  
  - job_name: 'Prod_YaoJet_dida_exporter'
    scrape_interval: 5s
    static_configs:
      - targets: ['116.62.41.156:9100']

  - job_name: 'Prod_YaoJet_nginx_exporter'
    scrape_interval: 5s
    static_configs:
      - targets: ['116.62.41.153:9100']


  - job_name: 'jumpserver_cadvisor'
    scrape_interval: 5s
    static_configs:
      - targets: ['cadvisor:8080']

  - job_name: 'Prod_DokcerRose_cadvisor'
    scrape_interval: 5s
    static_configs:
      - targets: ['10.64.4.155:8080']

  - job_name: 'Prod_DokcerJack_cadvisor'
    scrape_interval: 5s
    static_configs:
      - targets: ['10.64.4.146:8080']

  - job_name: 'jason_test_cadvisor'
    scrape_interval: 5s
    static_configs:
      - targets: ['101.37.77.148:8080']

  - job_name: 'tomcat1_cadvisor'
    scrape_interval: 5s
    static_configs:
      - targets: ['121.196.197.231:7070']

  - job_name: 'Prod_YaoJet_dida_cadvisor'
    scrape_interval: 5s
    static_configs:
      - targets: ['116.62.41.156:8080']

  - job_name: 'Pord_yaojet_nginx_cadvisor'
    scrape_interval: 5s
    static_configs:
      - targets: ['116.62.41.153:8080']

  - job_name: 'tomcat2_cadvisor'
    scrape_interval: 5s
    static_configs:
      - targets: ['121.43.163.194:7070']

  - job_name: 'prometheus'
    scrape_interval: 10s
    static_configs:
      - targets: ['localhost:9090']

  - job_name: 'pushgateway'
    scrape_interval: 10s
    honor_labels: true
    static_configs:
      - targets: ['pushgateway:9091']

  - job_name: "Prod_RDS"
    scrape_interval: 10s
    static_configs:
      - targets: ['116.62.41.156:9104']
  
  - job_name: "Prod_RDS_RO"
    scrape_interval: 10s
    static_configs:
      - targets: ['116.62.41.156:9105']
   
  - job_name: "jms_nginx"
    scrape_interval: 10s
    static_configs:
      - targets: ['10.64.4.156:9913']

  - job_name: "tomcat1-process"
    scrape_interval: 10s
    static_configs:
      - targets: ['121.196.197.231:9256']


alerting:
  alertmanagers:
  - scheme: http
    static_configs:
    - targets: 
      - 'alertmanager:9093'


